<c- p=3D"">,</c-> <c- t=3D"">'300'</c-><c- p=3D"">);</c-> <c- c1=3D"">// cl=
ears the canvas</c->
context<c- p=3D"">.</c->fillRect<c- p=3D"">(</c-><c- mf=3D"">0</c-><c- p=3D=
"">,</c-><c- mf=3D"">100</c-><c- p=3D"">,</c-><c- mf=3D"">50</c-><c- p=3D""=
>,</c-><c- mf=3D"">50</c-><c- p=3D"">);</c->
canvas<c- p=3D"">.</c->width <c- o=3D"">=3D</c-> canvas<c- p=3D"">.</c->wid=
th<c- p=3D"">;</c-> <c- c1=3D"">// clears the canvas</c->
context<c- p=3D"">.</c->fillRect<c- p=3D"">(</c-><c- mf=3D"">100</c-><c- p=
